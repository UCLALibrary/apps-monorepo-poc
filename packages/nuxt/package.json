{
  "name": "@apps-monorepo-poc/nuxt-module",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Nuxt module to enable autoimports",
  "repository": {
    "type": "git",
    "url": "https://github.com/UCLALibrary/apps-monorepo-poc",
    "directory": "packages/nuxt"
  },
  "author": "UCLA Library Application Team",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "build": "pnpm run dev:prepare",
    "release":"semantic-release",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint-config": "^0.7.5",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.2",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.19.0",
    "nuxt": "^3.15.2",
    "typescript": "catalog:",
    "vitest": "^3.0.4",
    "vue-tsc": "catalog:",
    "@apps-monorepo-poc/vue-component-library": "workspace:^"
  },
  "peerDependencies": {
    "@apps-monorepo-poc/vue-component-library": "workspace:^"
  },
  "engines": {
    "pnpm": "^9.12.1"
  }
}
